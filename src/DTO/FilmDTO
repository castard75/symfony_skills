<?php
namespace App\DTO;
use App\Entity\Film;
use App\Entity\Genre;
use App\Entity\Realisateur;
use Symfony\Component\Validator\Constraints as Assert;


class FilmDTO {
/**
     * @var Film|null
     */
    private $film;

    /**
     * @var Realisateur|null
     */
    private $realisateur;

        /**
     * @var Genre|null
     */
    private $genre;


   /**
     * @return Film|null
     */
    public function getFilm(): ?Film
    {
        return $this->film;
    }
    
    /**
     * @param Film $film
     * @return FilmDTO
     */
    public function setFilm(Film $film): self
    {
        $this->film = $film;
        return $this;
    }



    /**
     * @return Realisateur|null
     */
    public function getRealisateur(): ?Realisateur
    {
        return $this->film;
    }
    
    /**
     * @param Realisateur $realisateur
     * @return Realisateur
     */
    public function setRealisateur(Realisateur $realisateur): self
    {
        $this->realisateur = $realisateur;
        return $this;
    }


    
    /**
     * @return Genre|null
     */
    public function getGenre(): ?Genre
    {
        return $this->genre;
    }
    
    /**
     * @param Genre $genre
     * @return Genre
     */
    public function setGenrer(Genre $genre): self
    {
        $this->genre = $genre;
        return $this;
    }
    

}